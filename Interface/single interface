using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Interface
{
    class Program
    {
        static void Main(string[] args)
        {
            car2 c2 = new car2();
            c2.mycar();
        }
    }
    interface car1
    {
        void mycar();
    }
    class car2 : car1
    {
        public string color;
        public string name;
        public void mycar()
        {
            color = "White";
            name = "Audi";
            Console.WriteLine("color= " + color);
            Console.WriteLine("name= " + name);
            Console.ReadLine();
        }
    }
}
